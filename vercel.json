{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/calendar-event.js": {
      "maxDuration": 10
    },
    "api/calendar-event-update.js": {
      "maxDuration": 10
    },
    "api/calendar-event-delete.js": {
      "maxDuration": 10
    }
  }
}

